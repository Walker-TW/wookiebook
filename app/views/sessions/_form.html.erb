<%= form_with(scope: post, local: true) do |form| %>

  <p id="notice"><%= notice %></p>
  
  <p>
    <%= form.label :email %><br>
    <%= form.text_field :email, autofocus: true, placeholder: 'Email' %>
  </p>
 
  <p>
    <%= form.label :password %><br>
    <%= form.password_field :password, autocomplete: 'on', placeholder: 'Password' %>
  </p>
 
  <p>
    <%= form.submit 'HERRNNNGGGHHHHH', id: 'submitBtn' %>
  </p>

  <%= audio_tag("chewbacca.mp3", id: "chewbaccaPlay", preload: "true") %>
  <%= audio_tag("identification.mp3", id: "identification", preload: "true") %>

<% end %>